  const slider = new Swiper(".story__slider", {
  speed: 100,
  effect: 'fade',
  fadeEffect: {
    crossFade: true
  },
  watchSlidesProgress: true,
  loop: totalSlides > 1,
  autoplay: {
    delay: 4000,
